{
  "name": "preact-example",
  "version": "1.0.0",
  "description": "A simple example of a Webpack-generated Preact application",
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "lint": "eslint src --ext js,jsx",
    "build": "webpack",
    "start": "webpack-dev-server"
  },
  "dependencies": {
    "preact": "^10.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.0",
    "@ianwalter/eslint-config-react": "^2.0.0",
    "@ianwalter/renovate-config": "^1.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^2.0.1",
    "html-webpack-plugin": "^4.0.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": [
    "> 0.25%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter/eslint-config-react/preact"
    ]
  },
  "renovate": {
    "extends": [
      "@ianwalter"
    ]
  }
}
